 if [ ! -d maskcombo ]; then
  	mkdir maskcombo
 fi
 anat=$(ls | grep ^dt-neuro-mask.tag-anat.tag-)
  for folder in $anat; do
    cd ${anat}
    suffix=$(
        awk '
            match($0,/Sem|Plaus|Gram/) {
                print tolower(substr($0,RSTART,RLENGTH))
                exit
            }
        ' "_info.json"
    )
    echo $suffix
    cp "mask.nii.gz" ../../maskcombo/"mask_anat_$suffix.nii.gz" 
   done
